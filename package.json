{
    "name": "better-auth-expo-biometric",
    "version": "0.1.0",
    "description": "Secure biometric-protected session storage for better-auth on Expo/React Native",
    "license": "MIT",
    "author": "",
    "repository": {
        "type": "git",
        "url": "https://github.com/talon2295/better-auth-expo-biometric.git"
    },
    "homepage": "https://github.com/talon2295/better-auth-expo-biometric",
    "bugs": {
        "url": "https://github.com/talon2295/better-auth-expo-biometric/issues"
    },
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
    ],
    "scripts": {
