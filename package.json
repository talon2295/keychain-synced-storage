    "engines": {
        "node": ">=14.0.0"
    },
    "peerDependencies": {
        "@better-auth/core": ">=1.0.0",
        "@better-auth/expo": ">=1.0.0",
        "@react-native-async-storage/async-storage": ">=1.0.0",
        "react-native": ">=0.70.0",
        "react-native-aes-crypto": ">=2.0.0",
        "react-native-keychain": ">=8.0.0"
    },
    "dependencies": {
        "typescript": "^5.3.3"
